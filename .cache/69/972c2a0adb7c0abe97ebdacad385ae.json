\r\nhtml {\r\n  font-size: 62.5%;\r\n}\r\n\r\nbody {\r\n  margin: 0;\r\n  font-size: 2rem;\r\n  font-family: \"persianFont\", -apple-system, BlinkMacSystemFont, \"Segoe UI\",\r\n    Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n  color: #fff;\r\n  box-sizing: border-box;\r\n  background: url(images/background1.jpg);\r\n  background-size: cover;\r\n  filter: blur(15px);\r\n  transition: filter .5s;\r\n}\r\n\r\n.weather {\r\n  width: 80vw;\r\n  margin: 2rem auto;\r\n  padding: 20px 0;\r\n  white-space: nowrap;\r\n}\r\n\r\n.weather__search {\r\n  width: 100%;\r\n  height: 6vh;\r\n  padding: 15px 10px;\r\n  outline: none;\r\n  border: none;\r\n  background-color: rgba(255, 255, 255, 0.3);\r\n  border-radius: 10px;\r\n  font-size: 3rem;\r\n  color: #fff;\r\n  caret-color: #c1dd90;\r\n  font-family: \"persianFont\", -apple-system, BlinkMacSystemFont, \"Segoe UI\",\r\n    Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.weather__search:hover {\r\n  background-color: rgba(255, 255, 255, 0.5);\r\n}\r\n\r\n.weather__search:focus {\r\n  background-color: rgba(255, 255, 255, 0.5);\r\n  border: 3px solid #c1dd90;\r\n  box-shadow: 0 0 5px 3px #c1dd90;\r\n}\r\n.weather__search::placeholder {\r\n  font-size: 3rem;\r\n  color: #fff;\r\n  font-family: persianFont, -apple-system, BlinkMacSystemFont, \"Segoe UI\",\r\n    Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n}\r\n\r\n.weather__today {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin: 2rem 0 2rem;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.weather__image {\r\n  width: 25vmin;\r\n  align-self: center;\r\n}\r\n\r\n.weather__city {\r\n  font-size: 7rem;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.weather__day {\r\n  font-size: 4rem;\r\n  margin: 0 0 1rem;\r\n  font-weight: light;\r\n}\r\n\r\n.weather__indicators {\r\n  font-size: 3rem;\r\n  margin: 1rem 0;\r\n}\r\n\r\n.weather__indicators::before {\r\n  content: \"\";\r\n  width: 30px;\r\n  height: 30px;\r\n  display: inline-block;\r\n  vertical-align: middle;\r\n  margin-left: 0.5rem;\r\n  background-size: cover;\r\n}\r\n\r\n.weather__indicators--humidity::before {\r\n  background-image: url(images/humidity.png);\r\n}\r\n\r\n.weather__indicators--wind::before {\r\n  background-image: url(images/wind.png);\r\n}\r\n\r\n.weather__indicators--pressure::before {\r\n  background-image: url(images/pressure.png);\r\n}\r\n\r\n.weather__temp {\r\n  font-size: 10rem;\r\n  align-self: center;\r\n}\r\n\r\n.weahter__forecast {\r\n  text-align: center;\r\n  max-width: 100%;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.weather__forecast__item {\r\n  background-color: rgba(255, 255, 255, 0.2);\r\n  box-sizing: border-box;\r\n  padding: 6rem 2rem 2rem;\r\n  width: 20rem;\r\n  border-radius: 2rem;\r\n  flex-grow: 1;\r\n  transition: all 0.5s;\r\n}\r\n\r\n.weather__forecast__item:hover {\r\n  background-color: rgba(255, 255, 255, 0.5);\r\n  transform: translateY(-10px);\r\n  cursor: pointer;\r\n}\r\n\r\n\r\n.weather__forecast__item:active {\r\n  background-color: rgba(255, 255, 255, 0.8);\r\n  transform: scale(1.1);\r\n  cursor: pointer;\r\n}\r\n\r\n\r\n.weather__forecast__item:not(:first-child, :last-child) {\r\n  margin: 1rem 1rem;\r\n}\r\n\r\n.weather__forecast__item:first-child {\r\n  margin: 1rem 0 1rem 1rem;\r\n}\r\n\r\n.weather__forecast__item:last-child {\r\n  margin: 1rem 1rem 1rem 0;\r\n}\r\n\r\n.weather__forecast__icon {\r\n  width: 10rem;\r\n}\r\n\r\n.weather__forecast__day {\r\n  font-size: 3rem;\r\n  margin: 1rem 0;\r\n}\r\n\r\n.weather__forecast__temprature {\r\n  font-size: 3rem;\r\n  margin: 0 0 2rem;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ca5467c76d06cd667c52db3d63e7dfdf","cacheData":{"env":{}}}